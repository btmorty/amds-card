      "projectType": "library",
      "root": "libs/cards/card",
      "sourceRoot": "libs/cards/card/src",
      "prefix": "amds",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/cards/card"],
          "options": {
            "project": "libs/cards/card/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/cards/card/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/cards/card/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/cards/card"],
          "options": {
            "jestConfig": "libs/cards/card/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/cards/card/src/**/*.ts",
              "libs/cards/card/src/**/*.html"
            ]
          }
        }
      }
    },
