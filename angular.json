    "patient-info": {
      "projectType": "application",
      "root": "apps/patient-info",
      "sourceRoot": "apps/patient-info/src",
      "prefix": "amds",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/patient-info",
            "index": "apps/patient-info/src/index.html",
            "main": "apps/patient-info/src/main.ts",
            "polyfills": "apps/patient-info/src/polyfills.ts",
            "tsConfig": "apps/patient-info/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/patient-info/src/favicon.ico",
              "apps/patient-info/src/assets"
            ],
            "styles": ["apps/patient-info/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/patient-info/src/environments/environment.ts",
                  "with": "apps/patient-info/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "patient-info:build:production"
            },
            "development": {
              "browserTarget": "patient-info:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "patient-info:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/patient-info/src/**/*.ts",
              "apps/patient-info/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/patient-info"],
          "options": {
            "jestConfig": "apps/patient-info/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "patient-info-e2e": {
      "root": "apps/patient-info-e2e",
      "sourceRoot": "apps/patient-info-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/patient-info-e2e/cypress.json",
            "devServerTarget": "patient-info:serve:development",
            "tsConfig": "apps/patient-info-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "patient-info:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/patient-info-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
